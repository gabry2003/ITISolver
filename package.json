{
    "name": "itisolver",
    "version": "1.0.0",
    "description": "Programma che risolve le equazioni di ogni grado, le scompone e risolve altri problemi matematici con tutti i passaggi",
    "main": "index.js",
    "scripts": {
        "compile": "./node_modules/.bin/tsc",
        "start": "npm run compile && electron js/index.js",
        "dev": "APP_DEV=true npm start",
        "pack": "electron-builder --dir",
        "dist": "npm run compile && electron-builder",
        "generate-docs": "./node_modules/.bin/jsdoc ./* -c ./jsdoc.json -d ./docs/it/ -R README.md --verbose --recurse"
    },
    "build": {
        "win": {
            "asar": true,
            "icon": "images/icon.png",
            "target": [
                "msi"
            ]
        },
        "linux": {
            "asar": true,
            "icon": "./build/icon.icns",
            "target": [
                "deb",
                "rpm"
            ],
            "executableName": "ITISolver"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gabry2003/ITISolver.git"
    },
    "keywords": [
        "ruffini",
        "math",
        "solver"
    ],
    "author": {
        "name": "Gabriele Princiotta",
        "email": "gabriprinciott@gmail.com",
        "url": "https://gabriprinciott.it/"
    },
    "contributors": [{
            "name": "Gabriele Princiotta",
            "email": "gabriprinciott@gmail.com",
            "url": "https://gabriprinciott.it/"
        },
        {
            "name": "Antonino Faraci",
            "email": "antoninofaraci04@gmail.com",
            "url": "http://antoninofaraci04.altervista.org/"
        }
    ],
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/gabry2003/IntersezioniConGliAssi/issues"
    },
    "homepage": "https://github.com/gabry2003/IntersezioniConGliAssi#readme",
    "devDependencies": {
        "@types/electron": "^1.6.10",
        "@types/jquery": "^3.5.5",
        "@types/node": "^15.3.0",
        "electron": "^12.0.6",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "clean-jsdoc-theme": "^3.2.7",
        "console-error": "0.0.4",
        "electron-reload": "^1.5.0",
        "electron-settings": "^4.0.2",
        "jsdoc": "^3.6.6",
        "minami": "^1.2.3",
        "node-notifier": "^9.0.1",
        "path": "^0.12.7",
        "pug": "^3.0.2",
        "upath": "^2.0.1",
        "url": "^0.11.0"
    }
}