{
    "name": "intersezionecongliassi",
    "version": "1.0.0",
    "description": "Programma che risolve le equazioni di ogni grado utilizzando Ruffini",
    "main": "index.js",
    "scripts": {
        "start": "electron .",
        "dev": "APP_DEV=true electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "generate-docs": "./node_modules/.bin/jsdoc ./* -c ./jsdoc.json -d ./docs/it/ -R README.md --verbose --recurse"
    },
    "build": {
        "win": {
            "asar": true,
            "icon": "images/icon.png",
            "target": [
                "msi"
            ]
        },
        "linux": {
            "asar": true,
            "icon": "./build/icon.icns",
            "target": [
                "deb",
                "rpm"
            ],
            "executableName": "Intersezione con gli assi"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gabry2003/IntersezioniConGliAssi.git"
    },
    "keywords": [
        "ruffini",
        "math",
        "solver"
    ],
    "author": {
        "name": "Gabriele Princiotta",
        "email": "gabriprinciott@gmail.com",
        "url": "https://gabriprinciott.it/"
    },
    "contributors": [
        {
            "name": "Gabriele Princiotta",
            "email": "gabriprinciott@gmail.com",
            "url": "https://gabriprinciott.it/"
        },
        {
            "name": "Antonino Faraci",
            "email": "antoninofaraci04@gmail.com",
            "url": "http://antoninofaraci04.altervista.org/"
        }
    ],
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/gabry2003/IntersezioniConGliAssi/issues"
    },
    "homepage": "https://github.com/gabry2003/IntersezioniConGliAssi#readme",
    "devDependencies": {
        "electron": "^12.0.6"
    },
    "dependencies": {
        "clean-jsdoc-theme": "^3.2.7",
        "electron-reload": "^1.5.0",
        "jsdoc": "^3.6.6",
        "node-notifier": "^9.0.1",
        "path": "^0.12.7",
        "upath": "^2.0.1"
    }
}
